{
  "extends": ["next", "prettier", "next/core-web-vitals"],
  "plugins": ["react", "react-hooks", "prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "caughtErrors": "none",
        "ignoreRestSiblings": true,
        "vars": "all",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
